    <!-- START BLOCK : item -->
    <div class="{category} all-items">
        <div class="col-xs-6 col-sm-4 col-md-3 col-lg-3 pad-right-percent-2 pad-bottom-percent-1 pad-left-percent-0">
            <div class="item-team-content locationDescription arrow-box-clean gray-bg-3">
                <img class="meet-the-team-img-grid" src="{image}" alt="{name}"/>

                <div class="blue-navy-f-2 marg-top-fixed-15 marg-left-percent-5 pad-bottom-fixed-10 font-18 font-w-600">{title}</div>
                <div class="pad-bottom-fixed-15 marg-left-percent-5 font-13 black-f-1 font-w-600 l-h-small ">
                    {job-title}
                </div>
                <div class="marg-left-fixed-30 marg-right-fixed-30 pad-bottom-fixed-30 font-12 hidden">
                    {text}
                </div>
            </div>
        </div>
