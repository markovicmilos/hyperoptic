<!-- INCLUDE BLOCK : header -->

<!-- INCLUDE BLOCK : menu-maps -->

<div class="row container-fluid pad-left-percent-19 pad-right-percent-18 pad-bottom-percent-5 gray-bg-1">

    <p class=" marg-top-fixed-30 font-50 font-l-600 font-f-bariol-regular gray-f-4 ">Press</p>

    <div>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 pad-right-percent-2 pad-left-percent-0">
            <div id="locationDescription-ri" class=" locationDescription arrow-box-clean marg-top-fixed-10 gray-bg-3" style="height: 503px;">

                <div id="ldgh-1" class=" blue-navy-f-2 marg-top-fixed-15 marg-left-fixed-15  font-30 "> <a href="{main-link}">{main-title}</a>  </div>
                <div id="ldgp-2" class="marg-top-fixed-15 marg-left-fixed-15  font-14 ">{main-date}</div>
                <div id="ldgp-2" class="marg-top-fixed-15 marg-left-fixed-15 marg-right-fixed-15  font-18 ">
                    {main-text}
                    <a href="{main-link}" class="font-w-300 font-2 blue-f-4 font-3 font-w-100">Read more</a>
                </div>
            </div>
        </div>
    </div>

    <!-- START BLOCK : item -->
    <div>
        <div class="col-xs-12 col-sm-4 col-md-3 col-lg-3 pad-right-percent-2 pad-bottom-percent-1  pad-left-percent-0 " >
            <div id="locationDescription-ri" class=" locationDescription arrow-box-clean marg-top-fixed-10 gray-bg-3" style="height: 240px;" >
                <div id="ldgh-1" class=" blue-navy-f-2 marg-top-fixed-5 marg-left-fixed-15  font-16 ">  <a href="{link}">{title}</a> </div>
                <div id="ldgp-2" class="marg-top-fixed-5 marg-left-fixed-15  font-10 ">{date}</div>
                <div id="ldgp-2" class="marg-top-fixed-5 marg-left-fixed-15  font-11 marg-right-fixed-15" style="text-align: left;">
                    {text}
                    <a href="{link}" class="font-w-300 font-2 blue-f-4 font-3 font-w-100">Read more</a>
                </div>
            </div>
        </div>
    </div>
    <!-- END BLOCK : item -->
</div>
