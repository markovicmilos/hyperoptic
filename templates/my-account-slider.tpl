<div class="  col-xs-12 col-sm-12 col-md-12 col-lg-12 pad-all-0 marg-top-fixed-10 pad-bottom-fixed-60" ><!-- Carousel column start  --> 
	<div id="carousel-example-captions" class="carousel slide" data-ride="carousel"><!-- Carousel start  -->
		  <ol class="carousel-indicators">
			<li data-target="#carousel-example-captions" data-slide-to="0" class="active"></li>
			<li data-target="#carousel-example-captions" data-slide-to="1" class=""></li>
			<li data-target="#carousel-example-captions" data-slide-to="2" class=""></li>
		  </ol>
		  <div class="carousel-inner" role="listbox">
			<!-- START BLOCK : slider-item -->
			<a class="item {css}" href="#">
			<div>
				<img class="w-100" src="{base-url}/images/{image}" alt="Carousel 1">
				<div class="carousel-caption">
					<h3>{title}</h3>
					<p>{text}</p>
				</div>
			</div>
			</a>
			<!-- END BLOCK : slider-item -->
		  </div>
	</div>
</div>